{
    "builds": [{
        "src": "share/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
        "src": "build.sh"
      }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "share/wsgi.py"
        }
    ]
}